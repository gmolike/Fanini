    "eslint-plugin-storybook": "9.0.15",
    "globals": "^15.13.0",
    "lightningcss": "^1.30.1",
    "playwright": "^1.53.2",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "prop-types": "15.8.1",
    "rimraf": "^6.0.1",
    "storybook": "^9.0.15",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "uuid": "^11.1.0",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  },
  "typeCoverage": {
    "atLeast": 95,
    "strict": true,
    "ignoreCatch": true,
    "ignoreFiles": [
      "**/*.test.ts",
      "**/*.spec.ts"
    ],
    "cache": true,
    "detail": true
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
