{
  "name": "faninitiative-spandau-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host 0.0.0.0",
    "dev:debug": "vite --debug",
    "build": "tsc -b && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "preview": "vite preview",
    "preview:host": "vite preview --host 0.0.0.0",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:cache": "eslint src --cache --cache-location .eslintcache",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc -b",
    "type-check:watch": "tsc -b --watch",
    "test": "echo 'Tests werden später implementiert'",
    "test:watch": "echo 'Tests werden später implementiert'",
    "test:coverage": "echo 'Test Coverage wird später implementiert'",
    "clean": "rm -rf dist .cache",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "npm run clean && npm run clean:deps",
    "routes:generate": "pnpm exec tsx ../node_modules/@tanstack/router-plugin/dist/cli.js generate",
    "deps:update": "pnpm update --interactive",
    "deps:outdated": "pnpm outdated",
    "type-coverage": "type-coverage",
    "type-coverage:detail": "type-coverage --detail",
    "type-coverage:ci": "type-coverage --at-least 95",
    "sb": "storybook dev -p 6006",
    "sb:build": "storybook build",
    "sb:clean": "rimraf node_modules/.cache/storybook",
    "sb:init": "pnpm sb:clean && pnpm sb"
  },
  "dependencies": {
    "@fanini/charts": "workspace:*",
    "@axe-core/react": "^4.10.2",
    "@eslint/compat": "^1.3.1",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
