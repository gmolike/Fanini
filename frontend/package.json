{
  "name": "faninitiative-spandau-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host 0.0.0.0",
    "dev:debug": "vite --debug",
    "build": "tsc -b && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "preview": "vite preview",
    "preview:host": "vite preview --host 0.0.0.0",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc -b",
    "type-check:watch": "tsc -b --watch",
    "test": "echo 'Tests werden später implementiert'",
    "test:watch": "echo 'Tests werden später implementiert'",
    "test:coverage": "echo 'Test Coverage wird später implementiert'",
    "clean": "rm -rf dist .cache",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "npm run clean && npm run clean:deps",
    "routes:generate": "pnpm exec tsx ../node_modules/@tanstack/router-plugin/dist/cli.js generate",
    "deps:update": "pnpm update --interactive",
    "deps:outdated": "pnpm outdated"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@tanstack/react-router": "^1.91.4",
    "lucide-react": "^0.525.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.15.0",
    "@tanstack/router-devtools": "^1.91.4",
    "@tanstack/router-plugin": "^1.91.2",
    "@types/node": "^24.0.6",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.13.0",
    "lightningcss": "^1.30.1",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "vite": "^6.0.1"
  }
}
